<div class="page-header">
  <%= link_to translators_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_translator_path(@translator), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show translator</h1>
</div>

<dl class="dl-horizontal">
  <dt>Email:</dt>
  <dd><%= @translator.email %></dd>

  <dt>Firstname:</dt>
  <dd><%= @translator.firstname %></dd>

  <dt>Lastname:</dt>
  <dd><%= @translator.lastname %></dd>

</dl>
